{
  "name": "studypal",
  "version": "1.0.0",
  "description": "StudyPal Application",
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server:debug\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjuubot/study-pal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kjuubot/study-pal/issues"
  },
  "homepage": "https://github.com/kjuubot/study-pal#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "mongoose-data-seed": "^2.1.6",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-geolocated": "^3.0.1",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
